{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-unused-vars": "warn",
        "import/no-unresolved": "off",
        "no-undef": "warn",
        "semi": "off",
        "padded-blocks": "off",
        "operator-linebreak": ["error", "before"],
        "indent": ["error", 4],
        "comma-dangle": "off",
        "quotes": "off",
        "object-curly-spacing":"off"
    }
}
